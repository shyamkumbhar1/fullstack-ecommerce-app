{
  "name": "laravel-mysql-server-6yr",
  "type": "project",
  "description": "E-Commerce Platform Backend - Laravel + MySQL (2018)",
  "keywords": ["laravel", "mysql", "ecommerce", "2018", "legacy"],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "5.8.*",
    "laravel/tinker": "^1.0",
    "tymon/jwt-auth": "^1.0"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.5",
    "fzaninotto/faker": "^1.4"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

